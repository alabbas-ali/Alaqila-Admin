
.uplodeModal .nav-tabs-custom{
    margin-bottom: 0px; 
    box-shadow: none; 
}
.filesdisplaytab .content {
    height:150px;
    position: relative;
    border-radius: 0;
}

.mida{
    position: absolute;
    right: 2px;
    top: 32px;
    background-color: #fff; 
}

.form-group{
    position: relative;
}

.filesdisplaytab .content .thumbnail {
    position: absolute;
    overflow: hidden;
    top:0px;
    bottom:0px;
    left:0px;
    right:0px;
    margin: 5px;
    box-shadow:inset 0px 0px 15px rgba(0,0,0,0.1), inset 0px 0px 0px 1px rgba(0,0,0,0.05);
    background: #eee;
}

.filesdisplaytab .content .thumbnail.selected{
    border: 2px solid #F0BA75;
}

.filesdisplaytab .content .thumbnail .centered {
    position: absolute;
    top:0;
    left:0;
    height:100%;
    width: 100%;
    transform:translate(50%,50%);
}

.filesdisplaytab .content .thumbnail img {
    position: absolute;
    width: 100%;
    top: 0;
    left:0;
    transform:translate(-50%,-50%);
}

.filesdisplaytab .content .thumbnail video {
    position: absolute;
    width: 100%;
    top: 0;
    left:0;
    transform:translate(-50%,-50%);
}

.filesdisplaytab .content .thumbnail audio {
    position: absolute;
    width: 100%;
    top: 0;
    left:0;
    transform:translate(-50%,-50%);
}

#tab_2 {
    height: 300px;
    overflow: auto;
}